{
    "parser": "babel-eslint",
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "globals": {
        "window": true,
        "document": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "jest": true
    }
}